        <QueryParameters>
          <QueryParameter Name="@Par_Stud">
            <Value>=Parameters!Par_Stud.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Student.St_Lname, Student.St_Fname, Course.Crs_Id, Course.Crs_Name
FROM     Student INNER JOIN
                  Department ON Student.Dept_Id = Department.Dept_Id INNER JOIN
                  Stud_Course ON Student.St_Id = Stud_Course.St_Id INNER JOIN
                  Course ON Stud_Course.Crs_Id = Course.Crs_Id
where Student.St_Id=@Par_Stud</CommandText>
      </Query>
